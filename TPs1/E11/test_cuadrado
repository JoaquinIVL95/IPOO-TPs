<?php 

include 'cuadrado.php';


$objCuadrado = new Cuadrado([2,2], [2,4], [4,4], [4,2]);

echo $objCuadrado->area() . "\n";

$objCuadrado->ejecutarDesplazamiento(2);
echo $objCuadrado . "\n";

echo $objCuadrado->expandirVertices(2)  . "\n";
echo $objCuadrado->area() . "\n";
echo $objCuadrado . "\n";
